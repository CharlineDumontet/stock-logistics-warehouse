<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="product_putaway_wh_stock" model="product.putaway">
        <field name="name">Stock putaways</field>
    </record>
    <record id="putaway_strat_acid_category" model="stock.fixed.putaway.strat">
        <field name="category_id" ref="product_category_acid" />
        <field name="putaway_id" ref="product_putaway_wh_stock" />
        <field name="fixed_location_id" ref="location_bin_acids" />
    </record>
    <record id="putaway_strat_dangerous_category" model="stock.fixed.putaway.strat">
        <field name="category_id" ref="product_category_dangerous" />
        <field name="putaway_id" ref="product_putaway_wh_stock" />
        <field name="fixed_location_id" ref="location_bin_dangerous" />
    </record>
    <record id="putaway_strat_ice_cream_product" model="stock.fixed.putaway.strat">
        <field name="product_id" ref="product_product_ice_cream" />
        <field name="putaway_id" ref="product_putaway_wh_stock" />
        <field name="fixed_location_id" ref="stock.location_refrigerator_small" />
    </record>
    <record id="putaway_strat_ammonium_product" model="stock.fixed.putaway.strat">
        <field name="product_id" ref="product_product_ammonium_nitrate" />
        <field name="putaway_id" ref="product_putaway_wh_stock" />
        <field name="fixed_location_id" ref="location_freezer_dangerous" />
    </record>
    <record id="stock.stock_location_stock" model="stock.location">
        <field name="putaway_strategy_id" ref="product_putaway_wh_stock" />
    </record>
</odoo>
